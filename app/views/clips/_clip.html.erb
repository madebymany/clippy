<li>
	<%= link_to clip.url %>
	<p><%= clip.content %></p>
	<time class="timestamp">Posted <%= time_ago_in_words(clip.created_at) %> ago.</time>
	<% if current_user?(clip.user) %>
    <%= link_to "delete", micropost, method:  :delete,
                                     confirm: "You sure?",
                                     title:   clip.content %>
  <% end %>
</li>